<h2>Update the drone</h2>


<form method="POST" action="/drones/create">

<label>Name</label>
<input type="text" name="name" value="{{ name }}"/>

<label>Propellers</label>
<input type="text" name="propellers" value="{{ propellers }}"/>

<label>MaxSpeed</label>
<input type="text" name="maxSpeed" value="{{ maxSpeed }}"/>

<button type="submit">Save changes </button>
</form>